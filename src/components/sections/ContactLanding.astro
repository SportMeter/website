---
import Contact from './Contact.astro';
import Container from '../primitives/Container.astro';
import {type FormField } from "@/types/contact";

interface Props {
	title: string;
  message: string;
  privacyStatement: string;
	fields: FormField[];
}

const { title, fields, message, privacyStatement} = Astro.props;
console.log("test")
---

<div
  id="ContactLanding"
  class="justify-center flex flex-col m-4">

    <div class="items-center text-sm font-medium justify-center flex pt-20">
      <h2 class="text-[3.13rem] leading-none font-semibold my-1">{title}</h2>
    </div>
  <Contact message={message} privacyStatement={privacyStatement} fields={fields} />
</div>

